<strong class="home_name">物件一覧</strong>

<table>
  <tr>
    <th>物件名</th>
    <th>賃料</th>
    <th>住所</th>
    <th>築年数</th>
    <th>備考</th>
  </tr>

<% @properties.each do |property| %>
  <tr>
    <td><%= property.house_name %></td>
    <td><%= property.rent_money %></td>
    <td><%= property.address %></td>
    <td><%= property.house_age %></td>
    <td><%= property.note %></td>
    <td><%= link_to '確認する', property_path(property.id) %></td>
    <td><%= link_to "編集する", edit_property_path(property.id) %></td>
    <td><%= link_to '削除する', property_path(property.id), method: :delete, data: { confirm: '本当に削除していいですか？' } %></td>


  </tr>
<% end %>
</table>
<div class="registration"><%= link_to '物件を登録する', new_property_path %></div>



